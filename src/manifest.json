{
  "name": "Simple NTP",
  "version": "0.1",
  "description": "NTP replacement extension",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "topSites",
    "contextMenus",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": ["bg.js"]
  },
  "icons": {
    "16": "resources/icons/16.png",
    "48": "resources/icons/48.png",
    "128": "resources/icons/128.png"
  },
  "manifest_version": 2
}
